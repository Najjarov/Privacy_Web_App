@model Privacy_Web_App.Models.MapSpawnModel

@{

    Layout = "~/Views/Shared/_Layout.cshtml";

}


<div class="st-dark-bg" >
    <div class="st-height-b100 st-height-lg-b80"></div>
    <div class="container">
        
        <div class="st-section-heading st-style1">
            <h4 class="st-section-heading-title">@Model.map_name - Spawn Rates</h4>
            <h2 class="st-section-heading-subtitle" style="font-size:50px;">@Model.map_name - Spawn Rates</h2>
        </div>
        <div class="st-height-b25 st-height-lg-b25"></div>
    </div>
    <div class="container">
        @Html.Partial("/Views/Shared/_MapsLinksPartial.cshtml")
        @Html.Partial("/Views/Shared/_mapTabsPartial.cshtml")




        <div class="tab-content py-3 px-3 px-sm-0" id="nav-tabContent" style="position: relative;">

            
<div class="tab-pane fade show active" id="nav-spawns" role="tabpanel" aria-labelledby="nav-spawns-tab">
    <div class="nav nav-tabs nav-fill" id="nav-tab" role="tablist">
                    @if (Model.uav_spawn == "")
                    {
                        <a class="nav-item nav-link active" id="nav-console-tab" data-toggle="tab" href="#nav-console" role="tab" aria-controls="nav-console" aria-selected="true">Ring Console</a>
                    }
                    else
                    {
                        <a class="nav-item nav-link active" id="nav-uav-tab" data-toggle="tab" href="#nav-uav" role="tab" aria-controls="nav-uav" aria-selected="true">UAV</a>
                        <a class="nav-item nav-link" id="nav-console-tab" data-toggle="tab" href="#nav-console" role="tab" aria-controls="nav-console" aria-selected="true">Ring Console</a>
                    }
       
        <a class="nav-item nav-link" id="nav-hotzone-tab" data-toggle="tab" href="#nav-hotzone" role="tab" aria-controls="nav-hotzone" aria-selected="true">HotZone</a>
