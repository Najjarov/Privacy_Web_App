                    <a class="nav-item nav-link" id="nav-evo-tab" data-toggle="tab" href="#nav-evo" role="tab" aria-controls="nav-evo" aria-selected="true">Evo Harvester</a>
                </div>
    </div>
    <div class="tab-content py-3 px-3 px-sm-0" id="nav-tabContent">

                @if (Model.uav_spawn == "")
