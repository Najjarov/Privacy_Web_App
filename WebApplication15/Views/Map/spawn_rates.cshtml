                        <div class="container">
                            @if (Model.console_spawn == "")
                            {
                                <img src="/img/maps/tbd_map.png" />
                            }
                            else
                            {
                                <img src="/img/maps/@Model.console_spawn" />
                            }
                            <div class="st-height-b25 st-height-lg-b25"></div>
                        </div>
                    </div>
                }
                else
                {
                    <div class="tab-pane fade show active" id="nav-uav" role="tabpanel" aria-labelledby="nav-uav-tab">
            <div class="container">

                        @if (Model.uav_spawn == "")
                        {
                            <img src="/img/maps/tbd_map.png" />
